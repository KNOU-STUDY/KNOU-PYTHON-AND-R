   "source": [
    "birthyear <- function(){\n",
    "    age <- readline(prompt='Enter age: ')\n",
    "    curyear <- as.numeric(format(Sys.Date(), '%Y'))\n",
    "    b.year <- curyear~as.numeric(age)\n",
    "    list(born.year=b.year)\n",
    "}"
   ]
